.swiperCon swiper {
  width: 750rpx;
  height: 562.5rpx;
  text-align: center;
}
.swiperCon swiper image {
  width: 562.5rpx;
}
.swiperCon .pro_price {
  color: #f00;
  padding: 10px;
}
.swiperCon .pro_name {
  display: flex;
  justify-content: space-between;
  padding: 10px;
}
.swiperCon .pro_name .name {
  border-right: 2px solid #999;
  margin-right: 10px;
  font-size: 14px;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.swiperCon .pro_name .collect {
  width: 50px;
  flex-shrink: 0;
  text-align: left;
}
.swiperCon .proIntroduce {
  padding-bottom: 20px;
}
.swiperCon .proTab {
  position: fixed;
  left: 0;
  bottom: 0;
  display: flex;
  justify-content: space-between;
  background-color: #fff;
  width: 100%;
  align-items: center;
}
.swiperCon .proTab > view {
  text-align: center;
  height: 46px;
}
.swiperCon .proTab .tab_item {
  width: 16.66666667%;
  padding-top: 5px;
}
.swiperCon .proTab .incar {
  width: 25%;
  line-height: 46px;
  background-color: #FFA500;
  color: #fff;
}
.swiperCon .proTab .buy {
  width: 25%;
  line-height: 46px;
  background-color: #EB4450;
  color: #fff;
}
